{
  "lspconfig": {
    "lua_ls": {
      "Lua.hint.enable": true,
      "Lua.hint.setType": false,
      "Lua.hint.paramType": true,
      "Lua.hint.paramName": "Disable",
      "Lua.hint.semicolon": "Disable",
      "Lua.hint.arrayIndex": "Disable",
      "Lua.workspace.checkThirdParty": false,
      "Lua.codelens.enable": true,
      "Lua.completion.callSnippet": "Both",
      "Lua.completion.showParams": false,
      "Lua.doc.privateName": [
        "^-"
      ]
    },
    "basedpyright": {
      "basedpyright.analysis.diagnosticSeverityOverrides": {
        "reportUnusedImport": false,
        "reportUnusedVariable": false
      },
      "basedpyright.analysis.typeCheckingMode": "off"
    },
    "clangd": {
      "clangd.arguments": [
        "--ignore-unknown-compiler-flags"
      ]
    }
  }
}
